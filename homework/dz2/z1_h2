//Напишите функцию, которая моделирует бросание двух игральных кубиков (на каждом может выпасть от 1 до 6 очков)
#include<iostream>
using namespace std;
int main() {
	setlocale(LC_ALL,"Russian");
	srand(time(NULL));
	int a, b;
	a = 1 + rand() % 7;
	b = 1 + rand() % 7;
	cout <<"результат первого игрального кубика = " << a << endl;
	cout <<"результат второго игрального кубика = " << b << endl;
	return 0;
}
